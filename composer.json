{
  "name": "smartbooster/content-bundle",
  "type": "symfony-bundle",
  "description": "SMART BOOSTER content bundle",
  "keywords": [],
  "homepage": "https://github.com/smartbooster/content-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Bastien",
      "email": "nicolas.bastien@smartbooster.io",
      "homepage": "https://smartbooster.io"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "smartbooster/sonata-bundle": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "6.*"
  },
  "autoload": {
    "psr-4": {
      "Smart\\ContentBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smart\\ContentBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
